{
  "openapi": "3.1.0",
  "info": {
    "title": "GPT Access Code Checker",
    "version": "1.0.0"
  },
  "servers": [
    {
      "url": "https://xaydungthuonghieunhanh.com"
    }
  ],
  "paths": {
    "/gpt-codes.json": {
      "get": {
        "operationId": "getValidCodes",
        "summary": "Lấy danh sách mã truy cập GPT",
        "responses": {
          "200": {
            "description": "Danh sách mã hợp lệ",
            "content": {
              "application/json": {
                "schema": {
                  "type": "array",
                  "items": {
                    "type": "object",
                    "properties": {
                      "code": {
                        "type": "string"
                      },
                      "expires": {
                        "type": "string",
                        "format": "date"
                      }
                    },
                    "required": ["code", "expires"]
                  }
                }
              }
            }
          }
        }
      }
    }
  }
}
